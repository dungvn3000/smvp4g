<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='Factory'>
    <!-- Inherit the core Web Toolkit stuff.                        -->
    <inherits name='com.google.gwt.user.User'/>

    <inherits name="net.smvp.reflection.Reflection"/>
    <inherits name="net.smvp.aop.Aop"/>
    <inherits name="net.smvp.generator.Generator" />
    
    <generate-with class="net.smvp.factory.generator.aop.AopCreatorGenerated">
            <when-type-assignable class="net.smvp.factory.client.aop.AopCreator"/>
    </generate-with>

    <generate-with class="net.smvp.factory.generator.classtype.ClassCreatorGenerator">
        <when-type-assignable class="net.smvp.factory.client.classtype.ClassTypeCreator"/>
    </generate-with>

</module>
<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='Aop'>
    <!-- Inherit the core Web Toolkit stuff.                        -->
    <inherits name='com.google.gwt.user.User'/>

    <inherits name="com.smvp.generator.Generator" />

    <generate-with class="com.smvp.aop.generator.wrapper.ClassWrapperGenerator">
        <when-type-assignable class="com.smvp.aop.client.marker.Aspectable"/>
    </generate-with>

    <generate-with class="com.smvp.aop.generator.utils.AopUtilsGenerator">
        <when-type-assignable class="com.smvp.aop.client.utils.AopUtils"/>
    </generate-with>

    <generate-with class="com.smvp.aop.generator.factory.AopFactoryGenerated">
        <when-type-assignable class="com.smvp.aop.client.factory.AopFactory"/>
    </generate-with>

</module>

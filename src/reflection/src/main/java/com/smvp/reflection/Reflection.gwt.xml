<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='Reflection'>
    <!-- Inherit the core Web Toolkit stuff.                        -->
    <inherits name='com.google.gwt.user.User'/>

    <inherits name="com.smvp.generator.Generator" />
    <inherits name="com.smvp.aop.Aop"/>

    <!-- Generator -->
    <generate-with class="com.smvp.reflection.generator.factory.ClassFactoryGenerator">
        <when-type-assignable class="com.smvp.reflection.client.factory.ClassFactory"/>
    </generate-with>

    <generate-with class="com.smvp.reflection.generator.reflection.ClassTypeGenerator">
        <when-type-assignable class="com.smvp.reflection.client.marker.Reflectable"/>
    </generate-with>
</module>

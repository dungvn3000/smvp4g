    <inherits name='com.google.gwt.user.User'/>

    <inherits name="net.smvp.generator.Generator" />

    <!-- Generator -->
    <generate-with class="net.smvp.reflection.generator.reflection.ClassTypeGenerator">
        <when-type-assignable class="net.smvp.reflection.client.marker.Reflectable"/>
    </generate-with>
</module>
